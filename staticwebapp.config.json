{
  "routes": [
    {
      "route": "/",
      "statusCode": 301,
      "redirect": "https://www.abedi.dev/"
    },
    {
      "route": "/charlene-digital-ageism",
      "rewrite": "/charlene-digital-ageism.html"
    },
    {
      "route": "/*",
      "serve": "/index.html"
    }
  ],
  "navigationFallback": {
    "rewrite": "index.html",
    "exclude": ["/charlene-digital-ageism.html"]
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html",
      "statusCode": 200
    }
  }
}
